var init=function(a){var b=a?$("[data-dynamic]").not('[data-dynamic="'+a+'"]'):$("[data-dynamic]");b.each(function(a,b){var c=$(this).data("dynamic"),d=$(this).data("js"),e=$(this);c.length>0&&$.ajax({url:c,type:"GET",dataType:"html"}).done(function(a){if(e.html(a),e.hasClass("loading")&&e.removeClass("loading"),void 0!==d){var b=d.split(",");for(var c in b){var f=window[b[c]];"function"==typeof f&&f()}}}).fail(function(a){processError(a)})})},initBlock=function(a,b){var c=$('[data-dynamic="'+a+'"]'),d=c.attr("data-dynamic"),e=c.data("js"),f=c;d.length>0&&$.ajax({url:d,type:"GET",dataType:"html"}).done(function(c){if(f.html(c),void 0!==e){var d=e.split(",");for(var g in d){var h=window[d[g]];"function"==typeof h&&h()}}b&&init(a),$(document).trigger("EventSdcReady")}).error(function(a){processError(a)})},processError=function(a){var b=!1;try{b=$.parseJSON(a.responseText),b.sdc&&$.ajax({url:"https://auth.mail.ru/sdc",jsonp:"JSONP_call",data:{from:b.host},dataType:"jsonp",success:function(a){initBlock("/user/_auth",!0)},error:function(a){console.log(a)}})}catch(c){console.log(a,c)}};initBlock("/user/_auth",!0);